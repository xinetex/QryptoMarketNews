{
    "version": 2,
    "name": "qryptomarket-news",
    "builds": [
        {
            "src": "web/package.json",
            "use": "@vercel/next",
            "config": {
                "projectSettings": {
                    "rootDirectory": "web"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/web/$1"
        }
    ],
    "github": {
        "enabled": true
    }
}